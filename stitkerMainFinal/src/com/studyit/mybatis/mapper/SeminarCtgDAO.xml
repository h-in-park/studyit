<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<!-- ○ mapper 엘리먼트의 『namespace="패키지경로가 포함된 인터페이스"』 -->
<!--    - 인터페이스 내부에 메소드명 선언이 되어 있어야 한다. -->
<!--    - 메소드명과 『id="식별자"』가 일치해야 한다. -->
<mapper namespace="com.studyit.mybatis.ISeminarCtgDAO">

	 <!-- ○ 쿼리 내부에 있느 컬럼명의 정보가 -->
	 <!--    자료형 클래스(DTO) 멤버 변수(속성)와 일치하는 경우 -->
	 
	 <select id="scList" resultType="com.studyit.mybatis.SeminarCtgDTO">
		 SELECT SEMINAR_CATEGORY_CODE, SEMINAR_CATEGORY
		 FROM TBL_SEMINAR_CATEGORY
		 ORDER BY SEMINAR_CATEGORY_CODE
	 </select>
	 
</mapper>